{
  "name": "demo-chrome-devtools-front",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --host 0.0.0.0",
    "clean": "rimraf dist",
    "build:dev": "npm run clean && webpack --progress",
    "build": "npm run clean && NODE_ENV=production webpack --progress",
    "test:e2e": "node e2e",
    "test:jscoverage": "npm run build:dev && npm run test:e2e -- --build=false && rimraf coverage && nyc report --reporter=html && opn coverage/index.html",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.10",
    "lorem-ipsum": "^1.0.4",
    "nyc": "^11.7.1",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.43",
    "material-ui": "^1.0.0-beta.43",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.7.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
