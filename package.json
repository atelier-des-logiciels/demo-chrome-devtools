{
  "name": "demo-chrome-devtools",
  "version": "1.0.0",
  "description": "A demo showing chrome headless devtools abilities",
  "private": true,
  "author": "Guillaume ARM <trapcodien@gmail.com> (https://github.com/guillaumearm/)",
  "license": "MIT",
  "keywords": [
    "demo",
    "chrome",
    "devtools"
  ],
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --host 0.0.0.0",
    "build:dev": "webpack --progress",
    "build": "NODE_ENV=production webpack --progress",
    "start": "node server/cli.js",
    "test": "echo No unit tests specified",
    "test:e2e": "node e2e",
    "test:e2e:watch": "nodemon -i e2e/tmp e2e",
    "test:jscoverage": "npm run build:dev && npm run test:e2e -- --build=false && rimraf coverage && nyc report --reporter=html && opn coverage/index.html",
    "lint": "eslint .",
    "test:all": "npm run lint && npm un test:e2e && npm run test",
    "commit": "git-cz",
    "clean": "rimraf ./dist ./e2e/tmp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atelier-des-logiciels/demo-chrome-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/atelier-des-logiciels/demo-chrome-devtools/issues"
  },
  "homepage": "https://github.com/atelier-des-logiciels/demo-chrome-devtools#readme",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "cli-color": "^1.2.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "koa": "^2.5.0",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.10",
    "lorem-ipsum": "^1.0.4",
    "nyc": "^11.7.1",
    "opn-cli": "^3.1.0",
    "puppeteer": "^1.3.0",
    "puppeteer-to-istanbul": "^1.2.2",
    "rimraf": "^2.6.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.43",
    "material-ui": "^1.0.0-beta.43",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.7.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
